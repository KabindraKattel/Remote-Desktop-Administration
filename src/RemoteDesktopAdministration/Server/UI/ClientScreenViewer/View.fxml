<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.ScrollPane?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.VBox?>
<VBox xmlns:fx="http://javafx.com/fxml/1" prefHeight="690.0" prefWidth="775.0" xmlns="http://javafx.com/javafx/11.0.1">
    <padding>
        <Insets bottom="20.0" left="20.0" right="20.0" top="20.0"/>
    </padding>
    <ScrollPane fitToHeight="true" fitToWidth="true" maxHeight="1.7976931348623157E308"
                maxWidth="1.7976931348623157E308" VBox.vgrow="ALWAYS">
        <ImageView fx:id="screenImageView"/>
    </ScrollPane>
</VBox>
